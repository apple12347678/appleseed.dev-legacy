{
  "name": "appleseed.dev",
  "private": true,
  "description": "A landing page",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/apple12347678/appleseed.dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "tsc": "tsc",
    "lint": "eslint --cache src",
    "lint:strict": "yarn lint --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "yarn format --check",
    "format:write": "yarn format --write",
    "refactor": "yarn tsc && yarn lint:fix && yarn format:write",
    "clean-start": "yarn clean && yarn build && yarn serve"
  },
  "dependencies": {
    "@babel/core": "^7",
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@mdx-js/mdx": "^1.6.21",
    "@mdx-js/react": "^1.6.21",
    "@types/node": "^14.14.7",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.6.0",
    "gatsby-plugin-emotion": "^4.4.0",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-layout": "^1.4.0",
    "gatsby-plugin-manifest": "^2.6.0",
    "gatsby-plugin-mdx": "^1.5.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-react-i18next": "^0.0.27",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.9.0",
    "gatsby-plugin-typescript": "^2.6.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.6.1",
    "gatsby-transformer-sharp": "^2.7.0",
    "graphql": "^14",
    "i18next": "^19.8.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "typescript": "^3.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.1.2"
  }
}
